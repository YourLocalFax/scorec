
struct vec2(x : r32, y : r32)

proc -(a : vec2) -> vec2 =
   vec2(-a.x, -a.y)

proc +(a : vec2, b : vec2) -> vec2 =
   vec2(a.x + b.x, a.y + b.y)

proc -(a : vec2, b : vec2) -> vec2 =
   vec2(a.x - b.x, a.y - b.y)

proc *(a : vec2, b : vec2) -> vec2 =
   vec2(a.x * b.x, a.y * b.y)

proc *(a : vec2, b : r32) -> vec2 =
   vec2(a.x * b, a.y * b)

proc *(a : r32, b : vec2) -> vec2 =
   vec2(a * b.x, a * b.y)

proc /(a : vec2, b : vec2) -> vec2 =
   vec2(a.x / b.x, a.y / b.y)
